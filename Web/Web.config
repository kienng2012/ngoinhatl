			<add name="Ngoaingu" virtualUrl="^/Ngoaingu.htm" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Index.aspx?page=SubjectDetails&amp;serviceID=4" ignoreCase="true"/>
